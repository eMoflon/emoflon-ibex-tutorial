#using Hospital2Administration.*
#using AttrCondDefLibrary.*

#rule PatientInRoom #extends PatientToPatient #with Hospital2Administration


#source { 
	
	
	h:Hospital{
		-department->d
	}
	
	d:Department{
		-room->ro
	}
	ro:Room{
		++-lies->p1
	}
	
	
}

#target {
	
	v:Administration{
		++-patient->p2
	}
	}
	

#correspondence {
	
	htov:HospitalToAdministration{
		#src->h
		#trg->v
	}
	
}

#attributeConditions {
	
}
